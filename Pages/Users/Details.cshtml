@page
@model MISApp.Pages.Users.DetailsModel
@{
    ViewData["Title"] = "User Details";
}

<h2>User Details</h2>

<div>
    <h4>User</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Username
        </dt>
        <dd class="col-sm-10">
            @Model.User.Username
